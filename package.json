{
    "name": "fun-language",
    "displayName": "Fun Language",
    "description": "Fun language support",
    "version": "0.0.1",
    "publisher": "jBugman",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "license": "SEE LICENSE IN LICENSE",
    "contributes": {
        "languages": [{
            "id": "fun",
            "aliases": ["Fun", "fun"],
            "extensions": [".fun"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fun",
            "scopeName": "source.fun",
            "path": "./syntaxes/fun.tmLanguage.json"
        }]
    }
}
